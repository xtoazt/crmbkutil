{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["ES2020", "DOM"],
    "module": "None",
    "moduleResolution": "Node",
    "strict": true,
    "noImplicitAny": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "isolatedModules": false,
    "outDir": "dist",
    "rootDir": "src",
    "removeComments": true,
    "sourceMap": false
  },
  "include": ["src"]
}

